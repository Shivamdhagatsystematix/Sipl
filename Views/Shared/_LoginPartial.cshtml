@using Microsoft.AspNet.Identity
@if ( Session["Email"]!=null)
{
    using (Html.BeginForm("LogOut", "LogIn", FormMethod.Post, new { id = "logoutForm", @class = "navbar-right" }))
    {
                    @Html.AntiForgeryToken()

<ul class="nav navbar-nav navbar-right">
    <li>
        @Html.ActionLink("Hello " + User.Identity.GetUserName() + "!", "Index", "Manage", routeValues: null, htmlAttributes: new { title = "Manage" })
    </li>

    <li><a href="javascript:document.getElementById('logoutForm').submit()">Log off</a></li>
</ul>
    }
}
else
{
    <ul class="nav navbar-nav navbar-right">
        <li>@Html.ActionLink("Register", "Create", "NetUserView", routeValues: null, htmlAttributes: new { id = "registerLink" })</li>
        <li>@Html.ActionLink("Log In", "Login", "LogIn", routeValues: null, htmlAttributes: new { id = "loginLink" })</li>
    </ul>
}
