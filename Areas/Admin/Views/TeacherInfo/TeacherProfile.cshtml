@model Sipl.Models.NetUserViewModel
@{
    ViewBag.Title = "LoggedIn";
}

<fieldset>
    <legend>
        <text>
            @if (Session["Email"] != null)
            {
                <text>
                    Welcome!!!!! @Session["Email"].ToString()
                </text>
            }
        </text>
    </legend>
</fieldset>
<h2>Hello!! Sir</h2>
<div>
    <ul>
        <li>
    @Html.RouteLink("View Student ", "Default",
          new { action = "_TeacherPage", controller = "Admin/TeacherInfo", id = Model.UserId })
            </li>
        <li>

    @Html.RouteLink("View Profile ", "Default",
         new { action = "TeacherDetails", controller = "NetUserView", id = Model.UserId })
            </li>
        </ul>
</div>







