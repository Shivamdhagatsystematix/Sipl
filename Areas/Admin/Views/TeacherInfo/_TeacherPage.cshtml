@model IEnumerable<Sipl.Models.NetUserViewModel>
<h2>List of Students</h2>
<div>
    <a type="button" class="btn btn-primary btn-sm"
        href="@Url.Action("TeacherProfile","TeacherInfo", new { Id= ViewBag.SendId })">
         <span class="glyphicon glyphicon-list"></span>
         <span>Back to Main Menu</span>

    </a>
    @*@Html.RouteLink("Back to Menu ", "Default",
new { action = "TeacherProfile", controller = "Admin/TeacherInfo", id = Model.UserId })*@

</div>

<table class="table table-striped table ">
    <tr class="success">
        <th>
            @Html.DisplayName("UserId")
        </th>
        <th>
            @Html.DisplayName("FirstName")
        </th>
        <th>
            @Html.DisplayName("LastName")
        </th>
        <th>
            @Html.DisplayName("Course")
        </th>
       
        <th>
            @Html.DisplayName("DOB")
        </th>
        
    </tr>
    @foreach (var item in Model)
    {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UserId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LastName)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.CourseName)
        </td>
       <td>
            @Html.DisplayFor(modelItem => item.DOB)
        </td>
       
        <td>
            @Html.RouteLink("ViewProfile", "Default",
                    new { action = "RegisteredUserDetails", controller = "NetUserView", id = item.UserId  })
        </td>

    </tr>
    }
</table>








